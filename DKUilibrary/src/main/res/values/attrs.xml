<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundLayout">
        <attr name="round_strokeEnable" format="boolean" />
        <attr name="round_bgColor" format="color" />
        <attr name="round_strokeColor" format="color" />
        <attr name="round_strokeSize" format="dimension" />
        <attr name="round_layoutRadius" format="dimension" />
        <attr name="round_layoutTopLeftRadius" format="dimension" />
        <attr name="round_layoutTopRightRadius" format="dimension" />
        <attr name="round_layoutBottomLeftRadius" format="dimension" />
        <attr name="round_layoutBottomRightRadius" format="dimension" />
        <attr name="round_useGradientBg" format="boolean" />
        <attr name="round_gradientStartBg" format="color" />
        <attr name="round_gradientEndBg" format="color" />
        <attr name="round_gradientAngle" format="float" />
    </declare-styleable>
    <declare-styleable name="DKTextView">
        <attr name="dk_enableTextShadow" format="boolean" />
        <attr name="dk_textShadowRadius" format="dimension" />
        <attr name="dk_textShadowY" format="dimension" />
        <attr name="dk_textShadowX" format="dimension" />
