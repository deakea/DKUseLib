        <attr name="dk_useGradientColor" format="boolean"/>
        <attr name="dk_textGradientAngle" format="float"/>
        <attr name="dk_textStartColor" format="color"/>
        <attr name="dk_textEndColor" format="color"/>
        <attr name="dk_enableTextBorder" format="boolean"/>
